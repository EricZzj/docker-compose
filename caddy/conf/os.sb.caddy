telegram.os.sb {
    # 路由
    route /* {
        reverse_proxy https://api.telegram.org
    }

    import COMMON_CONFIG "/data/logs/telegram.os.sb.log"
}

418.os.sb {
    route / {
        trojan {
            user {$TROJAN_PASSWORD}
        }
        teapot
    }
    
    import COMMON_CONFIG "/data/logs/418.os.sb.log"
}
